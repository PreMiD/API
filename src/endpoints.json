{
	"default": [
		{
			"path": "/",
			"handler": "default"
		},
		{
			"path": "/ping",
			"handler": "ping"
		},
		{
			"path": "/crowdin",
			"handler": "crowdin"
		},
		{
			"path": ["/app/update", "/app/update/:bit", "/app/update/*"],
			"handler": "appUpdate"
		},
		{
			"path": "/firefox/updates",
			"handler": "ffUpdate"
		}
	],
	"v2": [
		{
			"path": "/v2/usage",
			"handler": "usage"
		},
		{
			"method": "post",
			"path": "/v2/science",
			"handler": "science"
		},
		{
			"path": "/v2/science/:identifier",
			"handler": "science"
		},
		{
			"method": "delete",
			"path": "/v2/science",
			"handler": "science"
		},
		{
			"path": [
				"/v2/presences",
				"/v2/presences/:presence",
				"/v2/presences/:presence/:file"
			],
			"handler": "presences"
		},
		{
			"path": "/v2/presenceUsage",
			"handler": "presenceUsage"
		},
		{
			"path": "/v2/versions",
			"handler": "versions"
		},
		{
			"path": [
				"/v2/langFile/list",
				"/v2/langFile/:project/:lang",
				"/v2/langFile/:project/list"
			],
			"handler": "langFile"
		},
		{
			"path": ["/v2/credits", "/v2/credits/:userId"],
			"handler": "credits"
		},
		{
			"path": ["/v2/betaAccess", "/v2/betaAccess/:userId"],
			"handler": "betaAccess"
		},
		{
			"path": ["/v2/alphaAccess", "/v2/alphaAccess/:userId"],
			"handler": "alphaAccess"
		},
		{
			"path": [
				"/v2/changelog",
				"/v2/changelog/:project",
				"/v2/changelog/:project/:version"
			],
			"handler": "changelog"
		},
		{
			"method": "post",
			"path": "/v2/addBetaUser/:token",
			"handler": "addBetaUser"
		},
		{
			"path": "/v2/betaUsers",
			"handler": "betaUsers"
		},
		{
			"path": "/v2/discordUsers",
			"handler": "discordUsers"
		},
		{
			"path": "/v2/partners",
			"handler": "partners"
		},
		{
			"method": "post",
			"path": "/v2/partners/apply",
			"handler": "partnerApply"
		},
		{
			"path": "/v2/jobs",
			"handler": "jobs"
		},
		{
			"method": "post",
			"path": "/v2/jobs/apply",
			"handler": "jobApply"
		},
		{
			"path": "/v2/jobs/benefits",
			"handler": "benefits"
		},
		{
			"path": "/v2/sponsors",
			"handler": "sponsors"
		},
		{
			"method": "post",
			"path": "/v2/applications/:token",
			"handler": "applications"
		},
		{
			"method": "post",
			"path": "/v2/tickets/:token",
			"handler": "tickets"
		},
		{
			"method": "post",
			"path": "/v2/downloads/:token/:item",
			"handler": "downloads"
		},
		{
			"method": "post",
			"path": "/v2/bugPost/:token",
			"handler": "bugPost"
		},
		{
			"path": "/v2/bugUserInfo/:token",
			"handler": "bugUserInfo"
		}
	]
}
